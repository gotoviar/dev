                        <default type="identity" />
                        <hovered type="scale" x="1.4" y="1.4" />
                        <active type="scale" x="0" y="1" />
                    </transform>
                  </image>
                  <frame>
                      <rounding>
                          <all v="10.0"/>
                      </rounding>
                      <color>
                        <default type = "UNIFORM_COLOR" first = "0x000000cc" />
                        <hovered type = "GRADIENT_VERTICAL" first = "0x000000FF" second="0xFFFFFFFF" />
                        <active type = "GRADIENT_VERTICAL" first = "0x000000FF" second="0xFFFFFFFF" />
                      </color>
                  </frame>
                  <textframe>
                      <rounding>
                          <all v="20.0"/>
                      </rounding>
                      <color>
                        <all type = "UNIFORM_COLOR" first = "0x4d66ffff" />
                      </color>
                  </textframe>
                  <text>
                      <align>
                          <all x="0.5" y="0.95" />
                      </align>
                      <color>
                          <all type = "UNIFORM_COLOR" first = "0xffffffff" />
                      </color>
                  </text>
                  <on_click>
                    <action order="1" name = "change_menu" src_name="rings"/>
                  </on_click>
                </imagebutton>          
