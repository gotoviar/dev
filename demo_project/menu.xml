<?xml version="1.0" encoding="utf-8"?>
<gtvr xmlns="http://www.w3.org/TR/html4/" version="1.0">
    <gui>
        <!--
                states:
                  default, hovered, active
                
                color types:
                  UNIFORM_COLOR
                  GRADIENT_HORIZONTAL
                  GRADIENT_VERTICAL
                  GRADIENT_DIAGONAL_LB_RT    // diagonal left, bottom -> right, top
                  GRADIENT_DIAGONAL_LT_RB    // diagonal left, top -> right, bottom
                  
                  first: used is used always
                  second: used when color is gradient 
            
                round_corners:
                    NONE
                    TOP_LEFT
                    TOP_RIGHT
                    BOTTOM_LEFT
                    BOTTOM_RIGHT
                    DISABLE
                    TOP
                    BOTTOM
                    LEFT
                    RIGHT
                    ALL
        
                draw_order:
                    DEFAULT,
                    FOREGROUND,
                    BACKGROUND
                
                dimension type:
                  ABSOLUTE
                  RELATIVE
                
                color format: RGBA
                
                easing types:
                  LINEAR
                  EASE_IN
                  EASE_OUT
                  EASE_IN_OUT
          -->
        <theme name="blue">
          <text_color>
            <all type = "UNIFORM_COLOR" first = "0xFFFFFFFF" />
          </text_color>

          <window_bg_color>
            <all type = "UNIFORM_COLOR" first = "0x000000BF" />
          </window_bg_color>

          <panel_bg_color>
            <all type = "UNIFORM_COLOR" first = "0x36363680" />
          </panel_bg_color>

          <border_color>
            <all type = "UNIFORM_COLOR" first = "0x4d66ffff" />
          </border_color>

          <frame_bg_color>
            <default type = "UNIFORM_COLOR" first = "0x36363680" />
            <hovered type = "GRADIENT_VERTICAL" first = "0xffffffb3" second = "0x36363680" />
            <active  type = "GRADIENT_VERTICAL" first = "0xffffffb3" second = "0x36363680" />
          </frame_bg_color>

          <button_frame_color>
            <default type = "UNIFORM_COLOR" first = "0x36363680" />
            <hovered type = "GRADIENT_VERTICAL" first = "0xffffffb3" second = "0x36363680" />
            <active  type = "GRADIENT_VERTICAL" first = "0xffffffb3" second = "0x36363680" />
          </button_frame_color>

          <window_padding>
            <all x="5.0" y="5.0" />
          </window_padding>

          <panel_padding>
            <all x="5.0" y="5.0"/>
          </panel_padding>

          <frame_padding>
            <all x="4.0" y="4.0" />
          </frame_padding>

          <window_rounding>
            <all v="17.0" />
          </window_rounding>
          
          <panel_rounding>
            <all v="10.0" />
          </panel_rounding>

          <frame_rounding>
            <all v="17.0" />
          </frame_rounding>

          <window_border_size>
            <default v="0.0" />
            <hovered v="5.0" />
            <active  v="5.0" />
          </window_border_size>

          <panel_border_size>
            <all v="4.0" />
          </panel_border_size>

          <frame_border_size>
            <default v="0.0" />
            <hovered v="4.0" />
            <active  v="4.0" />
          </frame_border_size>

          <window_title_align>
            <all x="0.5" y="0.5" />
          </window_title_align>

          <button_text_align>
            <all x="0.5" y="0.5" />
          </button_text_align>

          <button_image_align>
            <all x="0.5" y="0.5" />
          </button_image_align>

          <window_item_spacing>
            <all x="2.0" y="2.0" />
          </window_item_spacing>

          <panel_item_spacing>
            <all x="2.0" y="2.0" />
          </panel_item_spacing>

          <frame_item_spacing>
            <all x="2.0" y="2.0" />
          </frame_item_spacing>
        </theme>
        
        <animation name="anim1" duration="3" easing="LINEAR" />
        
        <menu name="home">                
            <window id="main_win" title="Test Window" theme="blue">
              <size type = "RELATIVE" x="100" y="100" />
              <position type = "ABSOLUTE" x="0" y="0" />
              <titlebar id="titlebar" title="Home" round_corners="TOP">
                  <size type = "RELATIVE" x="100" y="10" />
